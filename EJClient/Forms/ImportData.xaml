<Window x:Class="EJClient.Forms.ImportData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Import Data" Height="500" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <ListBox SelectionMode="Extended" x:Name="list">
           
        </ListBox>
        <CheckBox x:Name="chkClearDataFirst" Content="导入前先删除目标数据表的数据" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5,5,0,0"></CheckBox>
        <Button Content="确 定" x:Name="btnOK" Click="btnOK_Click_1" Grid.Row="1" HorizontalAlignment="Right" Width="60" VerticalAlignment="Top" Margin="0,5,75,0"></Button>
        <Button Content="取 消" x:Name="btnCancel" Grid.Row="1" IsCancel="True" HorizontalAlignment="Right" Width="60" VerticalAlignment="Top" Margin="0,5,5,0"></Button>
    </Grid>
</Window>
